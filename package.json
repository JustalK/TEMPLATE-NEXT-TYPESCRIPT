{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "build:docs": "npx jsdoc -c jsdoc.json -r ./src ./README.md",
    "test": "start-test dev 3000 cypress:run",
    "cypress:run": "nyc --reporter=lcov --reporter=html --reporter=json-summary --reporter=text-summary cypress run",
    "cypress:open": "cypress open",
    "cypress:coverage": "check-total --min 100"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.6",
    "babel-plugin-istanbul": "^6.1.1",
    "check-code-coverage": "^1.10.0",
    "jsdoc": "^3.6.7",
    "next": "12.0.1",
    "nyc": "^15.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.43.4",
    "start-server-and-test": "^1.14.0",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "*",
    "@next/eslint-plugin-next": "^12.0.1",
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "better-docs": "^2.3.2",
    "cypress": "*",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "typescript": "4.4.4"
  },
  "nyc": {
    "reporter": [
      "html",
      "lcov"
    ],
    "exclude": [
      "cypress/**"
    ]
  }
}
